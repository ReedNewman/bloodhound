{
    "description": "Bloodhound is a Node.js package that allows you to retrieve data from shipping carriers (DHL, FedEx, UPS, USPS) in a common format.",
    "dependencies": {
        "async": "~3.1.0",
        "moment-timezone": "~0.5.25",
        "node-geocoder": "~3.23.0",
        "petty-cache": "~2.4.1",
        "pitney-bowes": "~0.1.0",
        "shipping-fedex": "0.2.0",
        "tz-lookup": "~6.1.18",
        "us-states-normalize": "~1.0.0"
    },
    "devDependencies": {
        "coveralls": "*",
        "istanbul": "*",
        "mocha": "*"
    },
    "homepage": "https://github.com/mediocre/bloodhound",
    "keywords": [
        "fedex",
        "logistics",
        "newgistics",
        "pitney-bowes",
        "shipping",
        "tracking",
        "usps"
    ],
    "license": "Apache-2.0",
    "main": "index.js",
    "name": "@mediocre/bloodhound",
    "scripts": {
        "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- --exit --reporter spec test/* && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
        "test": "mocha --exit --reporter spec test/*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mediocre/bloodhound.git"
    },
    "version": "0.4.1"
}
